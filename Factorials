def factorial(n):
    result = 1
    for i in range(1, n+1):
        result *= i
    return result

# Example usage:
number = 5
print("Factorial of", number, "is", factorial(number))


#multiplication_table


def multiplication_table(n):
    for i in range(1, 11):
        print(f"{n} * {i} = {n*i}")

# Example usage:
number = 5
multiplication_table(number)

#other way

start=1
end =10

for i in range (start, end+1):
     results= 5 * i
     print  (f"5 * {i} = {results}")


